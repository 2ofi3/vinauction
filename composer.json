        "drupal/twig_tools": "^2.1",
        "drupal/twig_xdebug": "^1.4",
        "drupal/typed_data": "^1.0@beta"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "wikimedia/composer-merge-plugin": true
        },
        "platform": {
            "php": "8.1.32"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "www/"
            }
        },
        "merge-plugin": {
            "include": [
                "www/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/flag": {
                "str_replace-null-deprecation": "patches/3266574-2.patch"
            },
            "drupal/time_field": {
                "Time: function does not handle daylight savings time correctly": "./patches/time_field-dst-fix-3269318-2.patch"
            },
            "drupal/facets": {
                "Add option to or set default for searchbox widget to use contains rather than starts with": "https://www.drupal.org/files/issues/2022-07-15/facets-3264720-2.patch",
                "Facets searchbox widget doesn't work when multiple facets with this widget are shown": "./patches/facets-3300204-3.patch"
            },
            "drupal/menu_token": {
                "PHP 8.1: Deprecated function": "./patches/3292102-php8.1-string-cast.patch"
            },
            "drupal/paragraphs": {
                "The paragraph entity type did not specify a translation handler": "https://www.drupal.org/files/issues/2021-01-22/paragraph-entity-type-did-not-specify-translation-halder-3031598-45.patch"
            },
            "drupal/pathauto": {
                "Unable to download translation file": "https://www.drupal.org/files/issues/2020-04-27/3131286-3.patch"
            },
            "drupal/search_api": {
                "DatabaseExceptionWrapper while computing Views data for index - Serialization failure: 1213 Deadlock found": "https://www.drupal.org/files/issues/2025-03-26/3352150-15--add_field_instance_dependencies.patch"
            }
        }
    }
}
