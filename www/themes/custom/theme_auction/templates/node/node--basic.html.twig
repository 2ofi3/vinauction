{#
/**
 * @file
 * Theme override to display a node.
 *
 * @see template_preprocess_node()
 */
#}
{%
  set classes = [
  'c-node',
]
%}
{{ attach_library('classy/node') }}

{{ content.field_hero }}

<article{{ attributes.addClass(classes) }}>
  <div class="c-node__inner">
    {{ content|without('field_hero') }}
  </div>
</article>